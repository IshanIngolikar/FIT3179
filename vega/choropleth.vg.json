{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 600,
    "projection": {"type": "mercator"},
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/IshanIngolikar/FIT3179/refs/heads/w9/output.json",
          "format": {"type": "topojson", "feature": "SA4_2021_AUST_GDA2020"}
        },
        "transform": [
          {
            "lookup": "properties.STE_NAME21",
            "from": {
              "data": {
                "url": "https://raw.githubusercontent.com/IshanIngolikar/FIT3179/refs/heads/w9/population.csv",
                "format": {"type": "csv"}
              },
              "key": "State/Territory",
              "fields": ["Population"]
            }
          },
          {
            "lookup": "properties.STE_NAME21",
            "from": {
              "data": {
                "url": "https://raw.githubusercontent.com/IshanIngolikar/FIT3179/refs/heads/w9/hospital.csv",
                "format": {"type": "csv"}
              },
              "key": "State",
              "fields": ["Beds"]
            }
          },
          {
            "calculate": "datum.Beds / datum.Population",
            "as": "Beds_to_Population_Ratio"
          }
        ],
        "mark": {
          "type": "geoshape",
          "stroke": "white",
          "strokeWidth": 0.5
        },
        "encoding": {
          "color": {
            "field": "Beds_to_Population_Ratio",
            "type": "quantitative",
            "scale": {"scheme": "reds"},
            "legend": {"title": "Beds to Population Ratio"}
          },
          "tooltip": [
            {"field": "properties.STE_NAME21", "type": "nominal", "title": "State"},
            {"field": "Population", "type": "quantitative", "title": "Population", "format": ","},
            {"field": "Beds", "type": "quantitative", "title": "Total Beds", "format": ","},
            {"field": "Beds_to_Population_Ratio", "type": "quantitative", "title": "Beds to Population Ratio", "format": ".4f"}
          ]
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/IshanIngolikar/FIT3179/refs/heads/w9/output.json",
          "format": {"type": "topojson", "feature": "ne_10m_graticules_15"}
        },
        "mark": {"type": "geoshape", "stroke": "black", "strokeWidth": 0.5}
      },
      {
        "mark": {
          "type": "text",
          "fontSize": 10,
          "color": "black",
          "align": "center",
          "baseline": "middle"
        },
        "encoding": {
          "text": {"field": "properties.STE_NAME21", "type": "nominal"},
          "longitude": {"field": "properties.CENTROID_LON", "type": "quantitative"},
          "latitude": {"field": "properties.CENTROID_LAT", "type": "quantitative"}
        }
      }
    ]
  }
  